{
    "productName": "Vet",
    "asar": true,
    "appId": "com.vet.vite",
    "copyright": "Copyright © 2019-2023 KangBingKui",
    "directories": {
        "output": "release/${version}"
    },
    "electronDownload": {
        "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "publish": [{
        "provider": "generic",
        "url": ""
    }],
    "releaseInfo": {
        "releaseNotes": "版本更新的具体内容"
    },
    "files": [
        "dist",
        "dist-electron"
    ],
    "dmg": {
        "contents": [{
                "x": 410,
                "y": 150,
                "type": "link",
                "path": "/Applications"
            },
            {
                "x": 130,
                "y": 150,
                "type": "file"
            }
        ]
    },
    "mac": {
        "target": {
            "target": "default",
            "arch": [
                "arm64"
            ]
        },
        "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
        "icon": "icons/icon.icns",
        "darkModeSupport": true
    },
    "win": {
        "target": [
            "nsis",
            "msi"
        ],
        "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
        "icon": "icons/icon.ico"
    },
    "linux": {
        "icon": "icons"
    }
}