{
    "presets": [
        ["env", {
            "modules": "commonjs",
            "targets": {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
        }],
        "stage-2"
    ],
    "plugins": [
        "add-module-exports",
        "transform-runtime",
        "transform-decorators-legacy",
        ["component", {
            "libraryName": "element-ui",
            "styleLibraryName": "theme-chalk"
        }]
    ]
}