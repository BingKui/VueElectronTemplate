{
  "name": "vet-iview",
  "version": "1.0.0",
  "description": "vet项目的 iview 模板。",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "node ./electron/dev.js",
    "build": "node ./electron/build.js",
    "pack": "node ./electron/build.js && cross-env electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BingKui/VueElectronTemplate.git"
  },
  "keywords": [
    "Vue",
    "Electron",
    "Vuex",
    "Vue-Router",
    "Template"
  ],
  "author": "康兵奎",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BingKui/VueElectronTemplate/issues"
  },
  "homepage": "https://github.com/BingKui/VueElectronTemplate#readme",
  "build": {
    "productName": "Vet",
    "appId": "com.vet.template",
    "copyright": "康兵奎版权所有",
    "directories": {
      "output": "dist"
    },
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "publish": [{
      "provider": "generic",
      "url": ""
    }],
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [{
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "x64",
          "arm64"
        ]
      },
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
      "icon": "icons/icon.icns",
      "darkModeSupport": true
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ],
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
      "icon": "icons/icon.ico"
    },
    "linux": {
      "icon": "icons"
    }
  },
  "dependencies": {
    "@icon-park/vue": "^1.4.2",
    "axios": "^1.2.5",
    "fs-extra": "^11.1.0",
    "jszip": "^3.10.1",
    "nedb": "^1.8.0",
    "pinia": "^2.0.29",
    "shelljs": "^0.8.5",
    "view-ui-plus": "^1.3.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-scrolly": "^0.9.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-function-sent": "^7.18.6",
    "@babel/plugin-proposal-json-strings": "^7.18.6",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/plugin-proposal-throw-expressions": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-import": "^1.13.6",
    "cfonts": "^3.1.1",
    "chalk": "^5.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "css-loader": "^6.7.3",
    "del": "^7.0.0",
    "electron": "^22.1.0",
    "electron-builder": "^23.6.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "eslint": "^8.32.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^9.9.0",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "multispinner": "^0.2.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-loader": "^7.0.2",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-resources-loader": "^2.2.5",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.0.1",
    "vue-runtime-helpers": "^1.1.2",
    "vue-style-loader": "^4.1.3",
    "webpack": "^4.39.3",
    "webpack-build-notifier": "^2.3.0",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-notifier": "^1.14.1"
  }
}