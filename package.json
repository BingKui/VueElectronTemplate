{
  "name": "vet-iview",
  "version": "1.0.0",
  "description": "vet项目的iview模板。",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "node ./electron/dev.js",
    "build": "node ./electron/build.js",
    "pack": "node ./electron/build.js && cross-env electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BingKui/VueElectronTemplate.git"
  },
  "keywords": [
    "Vue",
    "Electron",
    "Vuex",
    "Vue-Router"
  ],
  "author": "康兵奎",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BingKui/VueElectronTemplate/issues"
  },
  "homepage": "https://github.com/BingKui/VueElectronTemplate#readme",
  "build": {
    "productName": "Vet",
    "appId": "com.vet.template",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "element-ui": "^2.12.0",
    "nedb": "^1.8.0",
    "v-contextmenu": "^2.8.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6",
    "vue-scrolly": "^0.9.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "cfonts": "^2.4.3",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "cross-spawn": "^7.0.0",
    "css-loader": "^2.1.1",
    "del": "^4.1.1",
    "electron": "^6.0.7",
    "electron-builder": "^20.39.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "multispinner": "^0.2.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "sass-resources-loader": "^2.0.0",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^2.1.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.3",
    "webpack-build-notifier": "^1.0.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
